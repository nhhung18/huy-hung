#include<iostream>
using namespace std;
void generate(int* arr, int n);
bool binaryString(int* arr, int n);
void output(int* arr, int n);
void input(int* arr, int n);
int main() {
    int t;
    cout<<"input t: ";
    cin>>t;
    for(int i = 0; i < t; ++i) {
        int n;
        int* arr;
        cin>>n;
        arr = new int[n];
        input(arr, n);
        generate(arr, n);
        delete[] arr;
    }
    return 0;
}

void input(int* arr, int n) {
    for(int i = 0; i < n; ++i) {
        cin >>arr[i];
    }
}

void output(int* arr, int n) {
    for(int i = 0; i < n; ++i) {
        cout<<arr[i];
    }
}

bool binaryString(int* arr, int n) {
    int i = n - 1;
    while(i >= 0 && arr[i] != 0) {
        arr[i] = 0;

    }
    if(i >= 0) {
        arr[i] = 1;
        return false;
    } else {
        return true;
    }
}

void generate(int* arr, int n) {
    bool isFinal = false;
    while(!isFinal) {
        output(arr, n);
        binaryString(arr, n);
    }
}